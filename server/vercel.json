{
    "version": 2,
    "builds": [
        {
            "src": "logbook_manager/wsgi.py",
            "use":"@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.11"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "logbook_manager/wsgi.py"
        }
    ]
}